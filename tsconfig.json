{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "esnext",
      "dom",
      "esnext.asynciterable"
    ],
    "importHelpers": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",
    "typeRoots": [
      "node_modules/@types"
    ],
    "stripInternal": true,
    "skipLibCheck": true,
    "noLib": false,
    "baseUrl": "./src",
    "allowSyntheticDefaultImports": true,
    "paths": {
      "~/*": ["./*"],
      "reis/*": ["./Core/Modules/*"]
    },
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowJs": true
  },
  "compileOnSave": false,
  "suppressImplicitAnyIndexErrors": true,
  "exclude": [
    "node_modules",
    "public",
    "build",
    "dist",
    "dev",
    "server.js",
    "tool.js",
    "worker.js",
    "applicationBack.js",
    "applicationFront.js"
  ]
}