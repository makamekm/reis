{
    "transform": {
        "^.+\\.(ts?|tsx?)$": "ts-jest"
    },
    "testURL": "http://127.0.0.1",
    "browser": true,
    "testRegex": "Test\\/.+\\.(ts?|tsx?)$",
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json",
        "node"
    ],
    "testPathIgnorePatterns": [
        "/node_modules/",
        "/example/"
    ]
}