{
  "name": "reiso-example-full",
  "version": "0.0.1",
  "description": "Reiso example",
  "private": true,
  "keywords": [
    "reiso",
    "example"
  ],
  "scripts": {
    "composer": "concurrently --kill-others \"node ../../composer.js outDir ./src/Composer dir ./src/Modules incDir '~/Modules' type gql Api\" \"node ../../composer.js outDir ./src/Composer dir ./src incDir '..' type pcss Components Modules\" \"node ../../composer.js outDir ./src/Composer dir ./src/Modules incDir '~/Modules' type ts Entity Reducer Query View Worker Tool WebHook Handler Server Client Both\"",
    "compile": "cross-env NODE_ENV=production node --max_old_space_size=8192 node_modules/.bin/parallel-webpack --no-stats -- --hide-modules --env.prod --env.maps",
    "prod": "concurrently --kill-others \"node dist/server\" \"node dist/worker\" \"node dist/handler\"",
    "prod_back": "concurrently --kill-others \"node dist/worker\" \"node dist/handler\"",
    "dev": "parallel-webpack --watch -p=2 -- --env.packages=server,client,style,worker,handler,tool",
    "dev_server": "concurrently --kill-others \"nodemon --watch './src' -e gql bare/gen.api.js\" \"node --max_old_space_size=2048 node_modules/.bin/parallel-webpack --watch -p=3 -- --env.packages=server,client,style\" \"nodemon --watch dev/server.js dev/server.js\" \"node bare/dev\"",
    "dev_back": "concurrently --kill-others \"node --max_old_space_size=2048 node_modules/.bin/parallel-webpack --watch -p=2 -- --hide-modules --env.packages=worker,handler\" \"nodemon dev/worker silent --watch dev/worker.js\" \"nodemon dev/handler --watch dev/handler.js\"",
    "dev_tool": "node --max_old_space_size=2048 node_modules/.bin/parallel-webpack -p=1 -- --hide-modules --env.packages=tool && node dev/tool",
    "concurrently": "concurrently",
    "seaport": "seaport listen 9090",
    "api_watch": "nodemon --watch './src' -e gql bare/gen.api.js",
    "api": "node_modules/.bin/apollo-codegen introspect-schema http://127.0.0.1:3000/graphql --output schema.json && node_modules/.bin/apollo-codegen generate src/**/*.gql --schema schema.json --target typescript --output src/ApiType.ts",
    "schema": "gqlschema http://127.0.0.1:3000/graphql -t",
    "test_client": "karma start karma.js",
    "test_unit": "parallel-webpack -- --env.packages=test && jasmine dev/test.js",
    "test": "parallel-webpack -- --env.packages=test && jasmine dev/test.js && karma start karma.js"
  },
  "author": "mkarpov",
  "license": "ISC",
  "devDependencies": {
    "@types/draft-js": "0.10.21",
    "@types/gm": "1.17.33",
    "@types/jquery": "3.3.1",
    "@types/react-color": "2.13.4",
    "@types/react-transition-group": "2.0.7",
    "@types/webpack": "4.1.2",
    "@types/graphql": "0.12.6",
    "@types/velocity-animate": "1.2.32",
    "@types/jasmine": "2.8.6",
    "@types/enzyme": "3.1.10",
    "apollo-codegen": "0.19.0",
    "autoprefixer": "8.2.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "browser-sync": "2.23.6",
    "concurrently": "3.5.1",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "5.1.4",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "font-awesome": "4.7.0",
    "git-revision-webpack-plugin": "2.5.1",
    "json-loader": "0.5.7",
    "less": "3.0.1",
    "less-loader": "4.1.0",
    "node-http-ping": "0.2.2",
    "parallel-webpack": "2.3.0",
    "pixrem": "4.0.1",
    "postcss-advanced-variables": "2.3.3",
    "postcss-apply": "0.9.0",
    "postcss-calc": "6.0.1",
    "postcss-css-variables": "0.8.1",
    "postcss-cssnext": "3.1.0",
    "postcss-custom-properties": "7.0.0",
    "postcss-import": "11.1.0",
    "postcss-loader": "2.1.3",
    "postcss-nested": "3.0.0",
    "postcss-nested-vars": "1.0.0",
    "postcss-pseudoelements": "5.0.0",
    "postcss-url": "7.3.1",
    "postcss-variables": "1.1.1",
    "raw-loader": "0.5.1",
    "source-map-support": "0.5.4",
    "style-loader": "0.20.3",
    "file-loader": "1.1.11",
    "text-loader": "0.0.1",
    "ts-loader": "4.1.0",
    "tslib": "1.9.0",
    "typescript": "2.7.2",
    "webpack": "4.2.0",
    "jasmine-core": "3.1.0",
    "jasmine": "3.1.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-jasmine": "1.1.1",
    "karma-webpack": "3.0.0",
    "karma-jasmine-html-reporter": "1.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jasmine-enzyme": "6.0.0",
    "babel-preset-airbnb": "2.4.0"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bull-arena": "2.4.2",
    "draft-js": "0.10.5",
    "fetch-everywhere": "1.0.5",
    "gm": "1.23.1",
    "jquery": "3.3.1",
    "moment": "2.21.0",
    "mysql": "2.15.0",
    "node-fetch": "2.1.1",
    "react-color": "2.14.0",
    "react-transition-group": "2.2.1",
    "reflect-metadata": "0.1.12",
    "regenerator-runtime": "0.11.1",
    "reiso": "0.2.12",
    "velocity-animate": "1.5.1"
  }
}
